#!/home/mizuki/Developments/smart-3d/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'stl-tools==0.3.0','console_scripts','image2stl'
__requires__ = 'stl-tools==0.3.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('stl-tools==0.3.0', 'console_scripts', 'image2stl')()
    )
